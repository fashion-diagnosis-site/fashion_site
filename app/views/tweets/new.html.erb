<h1>fashion_site</h1>
<h3>新規投稿ページ</h3>
 <%= form_for @tweet do |f| %>
 <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, :size => 30 %>
  </div>
  <div class="field">
    <%= f.label :about  %>
    <%= f.text_area :about, :size => "30x2" %>
  </div>
   <%= f.submit "投稿する" %>
<% end %>

<%= link_to "一覧に戻る",tweets_path %>